<h3>Rental Items</h3>
<ol>
  {{#each item as |item|}}

  <li>
    <article>
      <div class="row mb-2">
        <div class="col-md-6">
          <div class="card flex-md-row mb-4 box-shadow h-md-250">
            <div class="card-body d-flex flex-column align-items-start">
              <strong class="d-inline-block mb-2 text-primary">Boats</strong>
              <h3 class="mb-0">
                <a class="text-dark" href="#">Product Name</a>
              </h3>
              <div class="mb-1 text-muted">Subtitle</div>
              <p class="card-text mb-auto">
                "This is where the product description goes"
              </p>
              <a href="#">Call to Action</a>
            </div>
            <img class="card-img-right flex-auto d-none d-md-block" alt="alt text" style="width: 200px;"
              src="https://images.boats.com/resize/1/33/57/6753357_20180621100620356_1_XLARGE.jpg">
          </div>
        </div>
      </div>
    </article>
  </li>
  {{/each}}
</ol>